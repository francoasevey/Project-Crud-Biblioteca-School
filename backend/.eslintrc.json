{
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": "airbnb/base",
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["server", "node_modules"]
        }
      }
    },
    "rules": {
       "no-underscore-dangle": 0,
       "no-param-reassign": 0,
       "import/no-unresolved": 0
     }
  }
  